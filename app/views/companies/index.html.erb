<div class="cmp-index-container clearfix">
  <%= render partial: "shared/navbar-area", locals: {button_a: "Home", button_b: "Product", button_c: "Pricing", button_d: "Contact", button_e: "LOGOUT", button_f: "SIGN UP" } %>
  <div class="cmp-index-container-cont">
    <div class="cmp-index-content">
      <div class="cmp-index-cont-sides leftcontainer">
          <%= render partial: "companies/render-companies-index/windows-dinamic", locals: {companies: @companies, stations: @stations} %>
      </div>
    </div>
  </div>
  <% if current_user.role == "master" %>
    <%= render partial: "index-users-list" %>
  <% end %>
</div>

<%= javascript_include_tag "packs/idx-cmp-show" %>
