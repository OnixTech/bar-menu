
<%= simple_form_for(company) do |f| %>
  <div class="form-inputs cmp-new-form-inputs">
  <%= f.input :name, class:"form-item"%>
  <%= f.input :city, class:"form-item" %>
  <%= f.input :suburb, class:"form-item" %>
  <%= f.input :street, class:"form-item" %>
  <%= f.input :number, class:"form-item" %>
  <%= f.input :post, class:"form-item" %>
  </div>
  <div class="form-actions">
  <%= f.submit class:"New" %>
  </div>
<% end %>